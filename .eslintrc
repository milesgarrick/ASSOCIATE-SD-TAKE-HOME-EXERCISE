{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "node",
    "prettier"
  ],
  "rules": {
    "node/no-unsupported-features/es-syntax": [
      "error",
      {
        "version": ">=16.0.0",
        "ignores": ["modules"]
      }
    ],
    "prettier/prettier": "error"
  }
}
